{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": ".",
    "paths": {
      "@components/*" : ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@config": ["src/z/config"],
      "@z/*": ["src/z/*"],
      "@class/*": ["src/class/*"],
      "@content/*": ["src/content/*"],
      "@katex/*": ["public/katex/*"],
      "@utils/*": ["src/utils/*"],
    },
    "allowJs": true
  }
}